<UserControl x:Class="MusicTracker.Controls.NodeEnd"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MusicTracker.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"  MouseUp="UserControl_MouseUp" MouseDown="UserControl_MouseDown">
    <Ellipse StrokeThickness="1" Stroke="#888888">
        <Ellipse.Fill>
            <RadialGradientBrush>
                <GradientStop Offset="0" Color="#ffffff"/>
                <GradientStop Offset="1" Color="#aaaaaa"/>
            </RadialGradientBrush>
        </Ellipse.Fill>
    </Ellipse>
</UserControl>
